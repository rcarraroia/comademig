{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "ASAAS_API_KEY": "@asaas_api_key",
    "SUPABASE_URL": "@supabase_url", 
    "SUPABASE_ANON_KEY": "@supabase_anon_key"
  }
}